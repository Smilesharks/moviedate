	$("#menu-toggle").on('click touch', function (e) {
		e.preventDefault();
		$("#wrapper").toggleClass("toggled");
	});
	$(".nav-close").on('click touch', function (e) {
		e.preventDefault();
		if ($("#wrapper").hasClass("toggled")) {
				$("#wrapper").toggleClass("toggled");
